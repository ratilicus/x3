<script>
<name>adam.findwaresinsector</name>
<version>1</version>
<engineversion>53</engineversion>
<description>Reports the location of all ware containers and puts an Adv. Sat on them.</description>
<arguments>
</arguments>
<sourceplaintext>
<![CDATA[
  $Sector = [PLAYERSHIP] -> get sector
  $player.spd = [PLAYERSHIP] -> get current speed
  
  $menu = array alloc: size=0
  $dist.arr = array alloc: size=0
  
  $Xpos = $Sector -> get universe x index
  $Ypos = $Sector -> get universe y index
  $name = $Sector -> get name
  
  $Temp = find flying ware: sector=$Sector maintype=null subtype=null flags=[Find.Multiple] refobj=null maxdist=null maxnum=99, refpos=null
  $count = size of array $Temp
  while $count
    dec $count = 
    $obj = $Temp[$count]
    $name = $obj -> get name
    
    if $obj != null
      $num = $obj -> get flying ware count
      $dist = get distance between $obj and [PLAYERSHIP]
      
      if $num < 100
@       = wait 100 ms
        $dist2 = get distance between $obj and [PLAYERSHIP]
        $dist2 = $dist2 - $dist
        $dist2 = $dist2 * 1000 / $player.spd
        $name = sprintf: fmt='%s {%s}', $name, $dist2, null, null, null
      end
      $text = sprintf: fmt='-      %s %s - %s', $num, $name, $dist, null, null
      add non selectable menu item: $menu, text=$text
      $dist = - $dist
      append $dist to array $dist.arr
    end
  end
  
  add non selectable menu item: $menu, text=' '
  append -1 to array $dist.arr
  add non selectable menu item: $menu, text=' '
  append 1 to array $dist.arr
  
  $ships = find ship: sector=$Sector class or type=null race=null flags=[Find.Multiple] refobj=null maxdist=null maxnum=99 refpos=null
  $ships = sort array $ships
  $count = size of array $ships
  while $count
    dec $count = 
    $obj = $ships[$count]
    if $obj != null
      $race = $obj -> get owner race
      $name = $obj -> get name
      
      $dist = get distance between $obj and [PLAYERSHIP]
      if $race == Neutral Race
        $ind = '*  '
        $hull = $obj -> get hull percent
@       = wait 250 ms
        $dist2 = get distance between $obj and [PLAYERSHIP]
        $dist2 = $dist2 - $dist
        $dist2 = $dist2 * 400 / $player.spd
        $name = sprintf: fmt='%s %s%% {%s}', $name, $hull, $dist2, null, null
      else if $obj -> is [PLAYERSHIP] a enemy
        $ind = '!!!'
      else
        $ind = '     '
      end
      
      $text = sprintf: fmt='%s %s - %s [ %s ]', $ind, $name, $dist, $race, null
      add non selectable menu item: $menu, text=$text
      append $dist to array $dist.arr
    end
  end
  $menu = sort array: data=$menu sort values=$dist.arr
  
  add non selectable menu item: $menu, text=' '
  add non selectable menu item: $menu, text='--STATIONS--'
  
  $stations = find station: sector=$Sector class or type=null race=null flags=[Find.Multiple] refobj=null maxdist=null maxnum=99 refpos=null
  $count = size of array $stations
  while $count
    dec $count = 
    $obj = $stations[$count]
    if $obj != null
      $name = $obj -> get name
      add non selectable menu item: $menu, text=$name
    end
  end
  
  $resMenu = open custom info menu: title='Stuff in sector' description='Wares/Stations/Ships in sector' option array=$menu maxoptions=null
  
  
  return null
  
  
]]>
</sourceplaintext>
